клас Тварина:
    функція __init__(своє , прізвисько, вік):
        своє.прізвисько = прізвисько
        своє.вік = вік

    функція Голос(своє):
        підняти ПомилкаВіртуальногоМетоду("Метод реалізується у підкласах")


клас Собака(Тварина):
    функція Голос(своє):
        повернути "Гав!"


клас Кіт(Тварина):
    функція Голос(своє):
        повернути "Няв!"


пес = Собака("Лорд", 3)
кішка = Кіт("Плюша", 2)

друк (пес.прізвисько + " каже :" + пес.Голос())
друк (кішка.прізвисько + " каже " + кішка.Голос())